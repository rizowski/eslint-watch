#!/usr/bin/env node
require('@babel/polyfill');
require('source-map-support/register');
const path = require('path');

const esw = require(path.join(__dirname, '..', 'build'));

esw
  .run(process.argv)
  .then(console.log)
  .catch((err) => {
    console.error(err.message);
    process.exit(1);
  });
