#!/usr/bin/env node
require('core-js/stable');
require('source-map-support/register');
const path = require('path');

const esw = require(path.join(__dirname, '..', 'build'));

esw
  .run(process.argv)
  .then((result) => {
    if (result) {
      console.log(result);
    }
  })
  .catch((err) => {
    console.error(err.message);
    process.exit(1);
  });
